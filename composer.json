{
    "name": "Instrye/CloudflarePartners-ci4",
    "type": "project",
    "description": "CloudFlare Partner Console",
    "homepage": "https://github.com/Instrye/CloudflarePartners-ci4",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "kint-php/kint": "^2.1",
        "psr/log": "^1.1",
        "zendframework/zend-escaper": "^2.5"
    },
    "require-dev": {
        "codeigniter4/codeigniter4-standard": "^1.0",
        "mikey179/vfsStream": "1.6.*",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "CodeIgniter\\": "system/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "composer dump-autoload",
            "CodeIgniter\\ComposerScripts::postUpdate",
            "bash admin/setup.sh"
        ]
    },
    "authors": {
        "name": "Instrye",
        "homepage": "https://www.ddnpc.com"
    }
}
